{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "keychron-launcher",
  "version": "0.1.0",
  "identifier": "com.keychron-launcher.app",
  "build": {
    "beforeBuildCommand": "pnpm build"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "Keychron Launcher",
        "url": "https://launcher.keychron.com",
        "width": 1500,
        "height": 1000
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}